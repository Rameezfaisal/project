CREATE TABLE IF NOT EXISTS eng_test.rpt_omat_makenha_dmd_consumption_dtls (
  cmpprtno                STRING,
  last_modified_on        DATE,
  mfg_12mnths_consumption INT,
  mfg_5yrs_consumption    INT,
  mfg_dmd                 INT,
  nha                     STRING,
  sprs_5yrs_consumption   INT,
  sprs_dmd                INT
)
USING DELTA;


CREATE TABLE IF NOT EXISTS omat_test.rpt_omat_obpn_make_nha_dtls (
  cmpprtno         STRING,
  cmpqpa           INT,
  isprocessed      STRING,
  last_modified_on DATE,
  level            INT,
  matkl            STRING,
  mstae            STRING,
  nha              STRING
)
USING DELTA;


CREATE TABLE IF NOT EXISTS eng_test.rpt_omat_log_dtls (
  executed_on        TIMESTAMP,  -- SECONDDATE
  obprtno_cnt        INT,
  obprtno_nha_cnt    INT,
  obpr_cnt           INT,
  program_name       STRING
)
USING DELTA;

