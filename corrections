log_df = spark.createDataFrame(
    [(0, 0, 0, program_name)],
    schema=StructType([
        StructField("obprtno_cnt", IntegerType(), False),
        StructField("obprtno_nha_cnt", IntegerType(), False),
        StructField("obpr_cnt", IntegerType(), False),
        StructField("program_name", StringType(), False)
    ])
).withColumn("executed_on", F.current_timestamp())

(
    log_df
    .write
    .mode("append")
    .format("delta")
    .saveAsTable(tgt_log_tbl)
)