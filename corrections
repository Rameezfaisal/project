CREATE TABLE IF NOT EXISTS eng_test.rpt_omat_buynha_suppliers_plants (
  nha                      STRING,
  omat_selected_plant      STRING,
  omat_selected_supplier   STRING,
  omat_selected_suppliercd STRING,
  suppcd_1050              STRING,
  suppcd_1060              STRING,
  suppcd_1090              STRING,
  suppcd_1900              STRING,
  suppcd_2000              STRING,
  suppcd_3120              STRING,
  suppcd_po1               STRING,
  suppcd_po2               STRING,
  suppcd_pr                STRING,
  suppname_1050            STRING,
  suppname_1060            STRING,
  suppname_1090            STRING,
  suppname_1900            STRING,
  suppname_2000            STRING,
  suppname_3120            STRING,
  suppname_po1             STRING,
  suppname_po2             STRING,
  suppname_pr              STRING
)
USING DELTA;



CREATE TABLE IF NOT EXISTS eng_test.rpt_omat_log_dtls (
  executed_on        TIMESTAMP,  -- SECONDDATE
  obprtno_cnt        INT,
  obprtno_nha_cnt    INT,
  obpr_cnt           INT,
  program_name       STRING
)
USING DELTA;


