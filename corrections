final_dmd_cons_insert = (
    final_dmd_cons
    .select(
        F.col("cmpprtno"),
        F.col("lamqoh"),
        F.col("last_modified_on"),
        F.col("nha"),
        F.col("open_po_qty"),
        F.col("restricted_all_stock"),
        F.col("sprs_cons"),
        F.col("sprs_dmd")
    )
)

final_dmd_cons_insert.write.insertInto(tgt_dmd_cons)